{
  "manifest_version": 2,
  "name": "Lakkfjerner",
  "description": "Fjerner betalingsmurer fra nettaviser.",
  "Version": "1.0",

  "background": {
    "scripts": ["main.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "cookies",
    "*://*/*"
  ]
}
